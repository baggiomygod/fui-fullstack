{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "no-console": [false],
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-angle-bracket-type-assertion": false,
    "no-reference": false
  },
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "linterOptions": {
      "exclude": [
        "build/**/*.js",
        "node_modules/**/*.ts"
      ]
  }
}
